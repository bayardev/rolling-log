{
    "name": "bayardev/rolling-log",
    "description": "Smart Logger based on Events",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Massimiliano Pasquesi",
            "email": "massimiliano.pasquesi@bayard-presse.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "psr/log-implementation": "~1.0",
        "symfony/event-dispatcher": ">=2.8",
        "jms/serializer": ">=1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "doctrine/orm": "^2.5",
        "symfony/http-kernel":">=2.8"
    },
    "autoload": {
        "psr-4": {"Bayard\\RollingLog\\": "src/RollingLog"}
    },
    "autoload-dev": {
        "psr-4": {"Bayard\\RollingLog\\Tests\\": "tests/RollingLog"}
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "dev-0.x-dev"
        }
    }
}
